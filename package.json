  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travelist/ng2-directory-tree/issues"
  },
  "homepage": "https://github.com/travelist/ng2-directory-tree#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.3 || 2.0.0-rc.4 || 2.0.0-rc.5 || 2.0.0-rc.6 || 2.0.0-rc.7 || 2.0.0-rc.8 || 2.0.0-rc.9",
    "@angular/core": "2.0.0-rc.3 || 2.0.0-rc.4 || 2.0.0-rc.5 || 2.0.0-rc.6 || 2.0.0-rc.7 || 2.0.0-rc.8 || 2.0.0-rc.9",
    "rxjs": "*",

    "font-awesome": "^4.6.0"
  },
