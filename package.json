{
  "name": "ng2-directory-tree",
  "version": "0.3.0",
  "description": "Angular2 Directives for rendering Directory Tree",
  "main": "ng2-directory-tree.js",
  "typings": "ng2-directory-tree.d.ts",
  "keywords": [
    "angular2",
    "directory",
    "tree",
    "angularjs"
  ],
  "scripts": {
    "compile": "npm run typings && npm run compile:common",
    "compile:common": "./node_modules/.bin/tsc -p tsconfig.json",
